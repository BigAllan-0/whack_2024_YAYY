{
    "manifest_version": 3,
    "name": "Open Tabs Lister",
    "version": "1.0",
    "description": "Lists all open tabs in the browser",
    "permissions": ["tabs"],
    "background": {
    "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }